<openerp>
    <data>
        <record id="view_wizards_wizard" model="ir.ui.view">
            <field name="name">Skok - Procedures Form</field>
            <field name="model">skok.wizards.wizard</field>
            <field name="arch" type="xml">
                <form string="Procedures">
                    <group>
                        <group>
                            <field name="procedure" required="True"></field>
                        </group>
                        <group>
                            <field name="skok_id" readonly="True"></field>
                        </group>
                    </group>
                    <footer>
                        <button string="Run" type="object" name="run" class="oe_highlight"></button>
                         or 
                        <button special="cancel" string="Cancel" class="oe_link"></button>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</openerp>
