<openerp>
    <data>
        <!-- List Vew -->
        <record id="view_skok_owner_tree" model="ir.ui.view">
            <field name="name">Skok Owner</field>
            <field name="model">skok.owner</field>
            <field name="arch" type="xml">
                <tree string="Skok Owner">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <!-- Form view -->
        <record id="view_skok_owner_form" model="ir.ui.view">
            <field name="name">Skok Owner</field>
            <field name="model">skok.owner</field>
            <field name="arch" type="xml">
                <form string="Skok Owner">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" required="True"></field>
                            </group>
                            <group></group>
                        </group>
                        <group>
                            <group>
                                <field name="partner_id" invisible="True"></field>
                            </group>
                            <group></group>
                        </group>
                        <separator string="Notes"></separator>
                        <field name="notes"></field>
                    </sheet>
                </form>
            </field>
        </record>

    </data>
</openerp>